{
  "name": "online_store_backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "migrate-up": "db-migrate up",
    "migrate-down": "db-migrate down",
    "start": "npx tsc && node ./build/backend/server.js",
    "watch": "tsc-watch --esModuleInterop dev/server.ts --outDir ./build --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "dockerup": "docker-compose up ",
    "dockerdown": "docker-compose down -v",
    "test": "set ENV=test && npx tsc && db-migrate --env test up && jasmine && db-migrate reset --env test",
    "dockercycle": "docker-compose down -v && docker-compose up",
    "table": "set ENV=test && npx tsc dev/models/table_name.ts && node build/models/table_name.js"
  },
  "keywords": [],
  "author": "",
